{
  "name": "@forevolve/bootstrap-dark",
  "description": "A Bootstrap 4 Dark theme.",
  "version": "0.0.0",
  "prettier": "@forevolve/prettier-config",
  "scripts": {
    "build": "gulp",
    "test": "echo \"Info: no test specified\" && exit 0",
    "gulp-watch": "gulp watch",
    "browser-sync": "browser-sync start --server --files \"dist/css/*.css\"",
    "browser-sync-proxy": "browser-sync start --proxy \"http://localhost:7051/\" --files \"samples/razor-pages/wwwroot/css/*.css\"",
    "watch": "npm-run-all --parallel gulp-watch browser-sync-proxy"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ForEvolve/bootstrap-dark.git"
  },
  "keywords": [
    "bootstrap",
    "themes",
    "dark-theme"
  ],
  "author": "Carl-Hugo Marcotte",
  "contributors": [
    {
      "name": "Maciej Kr√ºger",
      "email": "mkg20001@gmail.com",
      "url": "https://mkg20001.io"
    }
  ],
  "license": "MIT",
  "dependencies": {
    "bootstrap": "^4.6.2",
    "jquery": "^3.5.1",
    "popper.js": "^1.16.1"
  },
  "files": [
    "scss/**/*.scss",
    "dist/**/*",
    "assets/**/*"
  ],
  "devDependencies": {
    "@forevolve/prettier-config": "^2.0.1",
    "autoprefixer": "^9.7.5",
    "browser-sync": "^2.26.7",
    "gulp": "^4.0.2",
    "gulp-clean-css": "^4.3.0",
    "gulp-cli": "^2.2.0",
    "gulp-concat-css": "^3.1.0",
    "gulp-postcss": "^8.0.0",
    "gulp-rename": "^2.0.0",
    "gulp-sass": "^5.0.0",
    "gulp-scss-lint": "^1.0.0",
    "gulp-sourcemaps": "^2.6.5",
    "node-sass": "^7.0.0",
    "npm-run-all": "^4.1.5",
    "npm-version": "^1.1.0"
  }
}
